<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />

        <!-- Bootstrap Core CSS -->
        <link href="{{ asset('BrioWeb/css/bootstrap/bootstrap.min.css')}}" rel="stylesheet" type="text/css">
        <!-- Bootstrap Theme CSS -->
        <link href="{{ asset('BrioWeb/css/bootstrap/bootstrap-theme.min.css')}}" rel="stylesheet" type="text/css">
        <!-- App Css -->
        <link href="{{ asset('BrioWeb/css/app/app.v1.css')}}" rel="stylesheet" type="text/css">
        <!-- Font Awesome CSS -->
        <link href="{{ asset('BrioWeb/css/fontawesome/font-awesome.min.css')}}" rel="stylesheet" type="text/css">
        <!-- BrioWeb Calendar CSS -->
        <link href="{{ asset('BrioWeb/css/plugins/calendar.min.css')}}" rel="stylesheet" type="text/css">

        <!-- Custom CSS -->
        <link href="{{ asset('css/custom.css') }}" rel="stylesheet" type="text/css">
    </head>
    <body>

        {% block header_sidebar %}{% endblock %}
        <div class="warper container-fluid">

        {% block body %}{% endblock %}
        {% block javascripts %}{% endblock %}
        </div>




        <!-- SCRIPTS-->
        <!-- BrioWeb JQuery -->
        <script src="{{ asset('BrioWeb/js/jquery/jquery-1.9.1.min.js') }}" type="text/javascript"></script>
        <!-- Custom JS -->
        <script src="{{ asset('js/custom.js') }}" type="text/javascript"></script>
        <!-- Bootstrap JS -->
        <script src="{{ asset('BrioWeb/js/bootstrap/bootstrap.min.js') }}" type="text/javascript"></script>
        <!-- BrioWeb Plugins -->
        <script src="{{ asset('BrioWeb/js/plugins/nicescroll/jquery.nicescroll.min.js') }}" type="text/javascript"></script>
        <!-- DataTables -->
        <script src="{{ asset('BrioWeb/js/plugins/datatables/jquery.dataTables.js') }}"></script>
        <script src="{{ asset('BrioWeb/js/plugins/datatables/DT_bootstrap.js') }}"></script>
        <script src="{{ asset('BrioWeb/js/plugins/datatables/jquery.dataTables-conf.js') }}"></script>
        <!-- BrioWeb Custom JS -->
        <script src="{{ asset('BrioWeb/js/app/custom.js') }}" type="text/javascript"></script>


    </body>
</html>
